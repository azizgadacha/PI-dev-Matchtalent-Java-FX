<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ModifierCategorieController">
   <children>
      <Label layoutX="100.0" layoutY="14.0" text="Modifier Categorie" />
      <Label layoutX="2.0" layoutY="66.0" text="Nom categorie" />
      <TextField fx:id="nameField" layoutX="107.0" layoutY="62.0" />
      <Button fx:id="saveButton" layoutX="51.0" layoutY="110.0" mnemonicParsing="false" onAction="#savecategorie" text="Enregistrer" />
      <Button fx:id="cancelButton" layoutX="182.0" layoutY="110.0" mnemonicParsing="false" onAction="#annulermodification" text="Annuler" />
   </children>
</AnchorPane>
